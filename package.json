{
  "name": "ld-pipeline",
  "description": "Linked Data pipelines for Basel-Stadt Open Data",
  "version": "2.0.0",
  "scripts": {
    "ogd:stdout": "barnard59 run --pipeline=http://opendatabs.github.com/ld-pipeline/ogd/stdout --format=text/turtle pipelines/ogd.ttl",
    "ogd:store": "barnard59 run --pipeline=http://opendatabs.github.com/ld-pipeline/ogd/store --format=text/turtle pipelines/ogd.ttl --variable=ENDPOINT_URL=$ENDPOINT_URL --variable=ENDPOINT_USER=$ENDPOINT_USER --variable=ENDPOINT_PASSWORD=$ENDPOINT_PASSWORD",
    "ogd:store-default": "barnard59 run --pipeline=http://opendatabs.github.com/ld-pipeline/ogd/store --format=text/turtle pipelines/ogd.ttl",
    "raum:stdout": "barnard59 run --pipeline=http://opendatabs.github.com/ld-pipeline/raum/stdout --format=text/turtle pipelines/raum.ttl",
    "raum:store": "barnard59 run --pipeline=http://opendatabs.github.com/ld-pipeline/raum/store --format=text/turtle pipelines/raum.ttl --variable=ENDPOINT_URL=$ENDPOINT_URL --variable=ENDPOINT_USER=$ENDPOINT_USER --variable=ENDPOINT_PASSWORD=$ENDPOINT_PASSWORD",
    "raum:store-default": "barnard59 run --pipeline=http://opendatabs.github.com/ld-pipeline/raum/store --format=text/turtle pipelines/raum.ttl",
    "rhein-ues-weil:corrected:stdout": "barnard59 run --pipeline=http://opendatabs.github.com/ld-pipeline/rhein-ues-weil/stdoutCorrected --format=text/turtle pipelines/rhein-ues-weil.ttl",
    "rhein-ues-weil:corrected:store-default": "barnard59 run --pipeline=http://opendatabs.github.com/ld-pipeline/rhein-ues-weil/storeCorrected --format=text/turtle pipelines/rhein-ues-weil.ttl",
    "rhein-ues-weil:raw:stdout": "barnard59 run --pipeline=http://opendatabs.github.com/ld-pipeline/rhein-ues-weil/stdoutRaw --format=text/turtle pipelines/rhein-ues-weil.ttl",
    "rhein-ues-weil:raw:store-default": "barnard59 run --pipeline=http://opendatabs.github.com/ld-pipeline/rhein-ues-weil/storeRaw --format=text/turtle pipelines/rhein-ues-weil.ttl",
    "rhein-ues-weil:static:stdout": "barnard59 run --pipeline=http://opendatabs.github.com/ld-pipeline/rhein-ues-weil/stdoutStatic --format=text/turtle pipelines/rhein-ues-weil.ttl",
    "rhein-ues-weil:static:store-default": "barnard59 run --pipeline=http://opendatabs.github.com/ld-pipeline/rhein-ues-weil/storeStatic --format=text/turtle pipelines/rhein-ues-weil.ttl",
    "mirror:swisstopo:stdout": "barnard59 run --pipeline=http://opendatabs.github.com/ld-pipeline/mirror/stdout --format=text/turtle pipelines/mirror.ttl",
    "mirror:swisstopo:store": "barnard59 run --pipeline=http://opendatabs.github.com/ld-pipeline/mirror/store --format=text/turtle pipelines/mirror.ttl --variable=SOURCE_ENDPOINT_URL=$SOURCE_ENDPOINT_URL --variable=ENDPOINT_URL=$ENDPOINT_URL --variable=ENDPOINT_USER=$ENDPOINT_USER --variable=ENDPOINT_PASSWORD=$ENDPOINT_PASSWORD",
    "mirror:swisstopo:store-default": "barnard59 run --pipeline=http://opendatabs.github.com/ld-pipeline/mirror/store --format=text/turtle pipelines/mirror.ttl"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:opendatabs/ld-pipeline.git"
  },
  "dependencies": {
    "@rdfjs/namespace": "^1.1.0",
    "@rdfjs/term-map": "^1.0.0",
    "barnard59": "^0.1.2",
    "barnard59-base": "^0.1.1",
    "barnard59-csvw": "^0.1.0",
    "barnard59-formats": "^0.1.2",
    "barnard59-ftp": "^0.1.1",
    "barnard59-graph-store": "^0.1.1",
    "barnard59-rdf": "^0.2.3",
    "barnard59-sparql": "^0.1.1",
    "glob": "^7.1.6",
    "rdf-ext": "^1.3.1",
    "rdf-stream-to-dataset-stream": "^1.0.0",
    "wellknown": "^0.5.0"
  },
  "devDependencies": {}
}
